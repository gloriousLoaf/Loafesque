    "editorSuggestWidget.border": "#15232d",
    "editorSuggestWidget.foreground": "#8FA8B7",
    "editorSuggestWidget.highlightForeground": "#FEE440",
    "editorSuggestWidget.selectedBackground": "#30343F",
