    "input.border": "#00BBF9",
    "input.foreground": "#FEE440",
    "input.placeholderForeground": "#8FA8B7",
    "inputOption.activeBorder": "#8dffff",
    "inputValidation.errorBackground": "#30343F",
    "inputValidation.errorBorder": "#FEE440",
    "inputValidation.infoBackground": "#30343F",
    "inputValidation.infoBorder": "#00BBF9",
    "inputValidation.warningBackground": "#30343F",
    "inputValidation.warningBorder": "#FEE440",
    // list
    "list.activeSelectionBackground": "#30343F",
    "list.activeSelectionForeground": "#8FA8B7",
    "list.dropBackground": "#00BBF9",
    "list.focusForeground": "#8FA8B7",
    "list.highlightForeground": "#FEE440",
    "list.hoverBackground": "#30343F",
    "list.hoverForeground": "#8FA8B7",
    "list.inactiveSelectionForeground": "#8FA8B7",
    // menu
    "menu.background": "#2A485A",
    // merge
    "merge.border": "#24566E",
    "merge.commonContentBackground": "#c97d0c",
    "merge.commonHeaderBackground": "#c97d0c",
    "merge.currentContentBackground": "#2F7366",
    "merge.currentHeaderBackground": "#2F7366",
    "merge.incomingContentBackground": "#185294",
    "merge.incomingHeaderBackground": "#185294",
    // notification colors - The colors below only apply for VS Code versions 1.21 and higher.
    "notificationCenter.border": "#FEE440",
    "notificationCenterHeader.foreground": "#8FA8B7",
    "notificationCenterHeader.background": "#122738",
    "notificationToast.border": "#FEE440",
    "notifications.foreground": "#8FA8B7",
    "notifications.background": "#122738",
    "notifications.border": "#FEE440",
    "notificationLink.foreground": "#FEE440",
    // panel
