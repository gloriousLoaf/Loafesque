    "editor.wordHighlightStrongBackground": "#ffffff21",
    // and this one is the rest of them
    "editor.wordHighlightBackground": "#ffffff21",
    "editorBracketMatch.background": "#18789C",
    "editorBracketMatch.border": "#FEE44080",
    "editorCodeLens.foreground": "#8FA8B7",
    "editorCursor.foreground": "#FEE440",
    "editorError.border": "#18789C",
