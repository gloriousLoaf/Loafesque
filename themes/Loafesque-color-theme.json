// This is based on Wes Bos' Cobalt2 Theme! I have left his original comments in place.
{
  "name": "Loafesque",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "function": {
      "foreground": "#FEE440"
    },
    "variable": "#fff",
    "interface": {
      // "foreground": "#ff0088",
      "foreground": "#F15BB5",
      "italic": true
    },
    "type": {
      // "foreground": "#ff0088",
      "foreground": "#F15BB5",
      "italic": true
    },
    // This colours both object definiton properties, as well as references. If the property does not exist, it will be white\.
    "property": "#00BBF9",
  },
  "colors": {
    // menu
    "menu.selectionForeground": "#fff",
    // activityBar
    "activityBar.background": "#122738",
    "activityBar.border": "#00F5D4",
    "activityBar.foreground": "#00F5D4",
    "activityBarBadge.background": "#FEE440",
    "activityBarBadge.foreground": "#000",
    // badge
    "badge.background": "#FEE440",
    "badge.foreground": "#000",
    // button
    "button.background": "#00BBF9",
    "button.foreground": "#fff",
    "button.hoverBackground": "#F8A07B",
    // contrast
    "contrastBorder": "#24566E",
    // debug
    "debugExceptionWidget.background": "#2A4557",
    "debugExceptionWidget.border": "#8FA8B7",
    "debugToolBar.background": "#2A4557",
    // description
    "descriptionForeground": "#8FA8B7",
    // diff
