    "peekViewEditorGutter.background": "#122738",
    "peekViewResult.background": "#15232d",
    "peekViewResult.fileForeground": "#8FA8B7",
    "peekViewResult.lineForeground": "#fff",
    "peekViewResult.matchHighlightBackground": "#00BBF9",
    "peekViewResult.selectionBackground": "#00BBF9",
    "peekViewResult.selectionForeground": "#fff",
    "peekViewTitle.background": "#15232d",
    "peekViewTitleDescription.foreground": "#8FA8B7",
    "peekViewTitleLabel.foreground": "#FEE440",
    // picker
    "pickerGroup.border": "#00BBF9",
    "pickerGroup.foreground": "#8FA8B7",
    // progressBar
    "progressBar.background": "#FEE440",
    // scrollbar
    "scrollbar.shadow": "#00000000",
    "scrollbarSlider.activeBackground": "#355166cc",
    "scrollbarSlider.background": "#00F5D480",
    "scrollbarSlider.hoverBackground": "#00F5D4cc",
    // selection
    "selection.background": "#027dff",
    // sidebar
    "sideBar.background": "#15232d",
    "sideBar.foreground": "#8FA8B7",
    "sideBarSectionHeader.background": "#30343F",
    "sideBarSectionHeader.foreground": "#8FA8B7",
    "sideBarTitle.foreground": "#8FA8B7",
    // statusBar
    "statusBar.background": "#15232d",
    "statusBar.debuggingBackground": "#15232d",
    "statusBar.debuggingBorder": "#FEE440",
    "statusBar.debuggingForeground": "#FEE440",
    "statusBar.foreground": "#8FA8B7",
    "statusBar.noFolderBackground": "#15232d",
    "statusBar.noFolderBorder": "#00BBF9",
    "statusBar.noFolderForeground": "#8FA8B7",
    "statusBarItem.activeBackground": "#00BBF9",
    "statusBarItem.hoverBackground": "#00BBF9",
    "statusBarItem.prominentBackground": "#15232d",
    "statusBarItem.prominentHoverBackground": "#00BBF9",
    // tab
    "tab.activeBackground": "#30343F",
    "tab.activeForeground": "#fff",
    "tab.border": "#15232D",
    "tab.activeBorder": "#FEE440",
    "tab.inactiveBackground": "#122738",
    "tab.inactiveForeground": "#8FA8B7",
    "tab.unfocusedActiveForeground": "#8FA8B7",
    "tab.unfocusedInactiveForeground": "#8FA8B7",
