    "gitDecoration.deletedResourceForeground": "#ff628c",
    "gitDecoration.untrackedResourceForeground": "#00F5D4",
    "gitDecoration.ignoredResourceForeground": "#808080",
    "gitDecoration.conflictingResourceForeground": "#FF7200",
    // textBlockQuote
    "textBlockQuote.background": "#30343F",
    "textBlockQuote.border": "#00BBF9",
    "textCodeBlock.background": "#30343F",
    "textLink.activeForeground": "#00BBF9",
    "textLink.foreground": "#00BBF9",
    "textPreformat.foreground": "#FEE440",
    "textSeparator.foreground": "#00BBF9",
    "titleBar.activeBackground": "#15232D",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#2A4557",
    "titleBar.inactiveForeground": "#ffffff33",
    "walkThrough.embeddedEditorBackground": "#00BBF9",
    "welcomePage.buttonBackground": "#2A4557",
    "welcomePage.buttonHoverBackground": "#00BBF9",
    "widget.shadow": "#00000026"
